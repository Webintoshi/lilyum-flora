{
  "project_name": "Lilyum Flora",
  "project_type": "e-commerce",
  "frontend_tech_stack": {
    "framework": "React",
    "version": "^18.3.1",
    "language": "TypeScript",
    "routing": "react-router-dom ^7.3.0",
    "build_tool": "Vite ^6.3.5",
    "styling": "TailwindCSS ^3.4.17",
    "state_management": "Zustand ^5.0.3"
  },
  "backend_tech_stack": {
    "runtime": "Node.js",
    "framework": "Express ^4.21.2",
    "language": "TypeScript"
  },
  "database": {
    "type": "Supabase",
    "version": "^2.90.1"
  },
  "dev_server": {
    "frontend_port": 5173,
    "backend_port": 3001,
    "dev_command": "npm run dev"
  },
  "key_features": {
    "user_features": [
      "Product catalog browsing",
      "Product detail pages",
      "Shopping cart",
      "Checkout process",
      "User authentication (login/register)",
      "User profile management",
      "Order history",
      "Order tracking",
      "Wishlist",
      "Address management",
      "Contact form"
    ],
    "admin_features": [
      "Admin login",
      "Dashboard",
      "Product management",
      "Order management",
      "Customer management",
      "Category management",
      "Banner management",
      "SEO settings",
      "Scraper tools",
      "Settings"
    ],
    "ui_features": [
      "Flower animation effects",
      "Magic dust button effects",
      "Side cart drawer",
      "Bottom navigation",
      "Floating buttons",
      "Responsive design"
    ]
  },
  "api_endpoints": [
    "/api/auth",
    "/api/admin",
    "/api/banners",
    "/api/categories",
    "/api/customers",
    "/api/dashboard",
    "/api/heroBanners",
    "/api/orders",
    "/api/products",
    "/api/scrapers",
    "/api/seo",
    "/api/settings"
  ],
  "main_pages": [
    "/ - Home",
    "/catalog - Product catalog",
    "/product/:id - Product detail",
    "/cart - Shopping cart",
    "/checkout - Checkout page",
    "/login - User login",
    "/register - User registration",
    "/profile - User profile",
    "/wishlist - User wishlist",
    "/contact - Contact page",
    "/about - About page",
    "/landscaping - Landscaping services",
    "/admin/* - Admin panel routes"
  ],
  "third_party_services": [
    "Supabase (authentication & database)",
    "Vercel (deployment)"
  ]
}
