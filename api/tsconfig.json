{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["ES2020"],
    "module": "ESNext",
    "moduleResolution": "bundler",
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": false,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": false,
    "outDir": "../dist/api",
    "rootDir": ".",
    "noEmit": false,
    "types": ["node", "express"]
  },
  "include": [
    "."
  ],
  "exclude": [
    "node_modules"
  ]
}
